{
  "name": "@uniformdev/unistore",
  "version": "17.1.0",
  "license": "Unlicense",
  "private": true,
  "engines": {
    "npm": ">=8",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s pull:manifest dev:next",
    "dev:next": "next dev",
    "build": "run-s pull:manifest build:next",
    "build:next": "next build",
    "pull:manifest": "uniform context manifest download --output ./src/lib/context/manifest.json",
    "start": "next start",
    "ci:build": "npm run pull:manifest && next build",
    "pull": "run-s pull:canvas pull:context",
    "pull:canvas": "run-s pull:components pull:compositions",
    "pull:compositions": "uniform presentation composition pull ./data/canvas/compositions",
    "pull:components": "uniform presentation component pull ./data/canvas/components",
    "push": "run-s push:components push:compositions",
    "push:compositions": "uniform presentation composition push ./data/canvas/compositions",
    "push:components": "uniform presentation component push ./data/canvas/components",
    "pull:context:signals": "uniform context signal pull ./data/context/signals",
    "pull:context:enrichments": "uniform context enrichment pull ./data/context/enrichments",
    "pull:context:aggregates": "uniform context aggregate pull ./data/context/aggregates",
    "pull:context:quirks": "uniform context quirk pull ./data/context/quirks",
    "pull:context:tests": "uniform context test pull ./data/context/tests",
    "pull:context": "run-s pull:context:signals pull:context:enrichments pull:context:tests pull:context:aggregates",
    "push:context:signals": "uniform context signal push ./data/context/signals",
    "push:context:enrichments": "uniform context enrichment push ./data/context/enrichments",
    "package": "run-s package:canvas package:context",
    "package:canvas": "run-s package:components package:compositions",
    "package:compositions": "uniform presentation composition pull ./data/unistore.yaml",
    "package:components": "uniform presentation component pull ./data/unistore.yaml",
    "package:context:signals": "uniform context signal pull ./data/unistore.yaml",
    "package:context:enrichments": "uniform context enrichment pull ./data/unistore.yaml",
    "package:context:aggregates": "uniform context aggregate pull ./data/unistore.yaml",
    "package:context:quirks": "uniform context quirk pull ./data/unistore.yaml",
    "package:context:tests": "uniform context test pull ./data/unistore.yaml",
    "package:context": "run-s package:context:signals package:context:enrichments package:context:tests package:context:aggregates",
    "lint": "eslint --ext .js --ext .jsx --ext .tsx --ext .ts ./",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "fix-js-lint": "eslint --fix \"./**/*.{ts,tsx}\"",
    "fix": "npm run format && npm run fix-js-lint",
    "start:stackblitz": "npm install && next dev"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "@uniformdev/canvas": "^17.1.0",
    "@uniformdev/canvas-bigcommerce": "^17.1.0",
    "@uniformdev/canvas-react": "^17.1.0",
    "@uniformdev/cli": "^17.1.0",
    "@uniformdev/context": "^17.1.0",
    "@uniformdev/context-devtools": "^17.1.0",
    "@uniformdev/context-next": "^17.1.0",
    "@uniformdev/context-react": "^17.1.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "lodash": "^4.17.21",
    "next": "^12.2.5",
    "next-themes": "0.0.15",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-multi-carousel": "^2.6.5",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.2",
    "sass": "^1.38.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "tailwindcss-animation-delay": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.16.5",
    "@netlify/plugin-nextjs": "^4.9.3",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/cookie": "^0.4.1",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-transition-group": "^4.4.4",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bulma": "^0.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.1-rc.0-next-f2a59df48-20211208",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "stackblitz": {
    "startCommand": "npm run start:stackblitz",
    "compileTrigger": "save"
  }
}
